{{#eq type 'Stateful'}}
/**
 * Component
 */
class {{pascalCase name}}Component extends React.Component {
{{#if isConnected}}{{#each connectedFunctions}}{{#eq this 'componentWillReceiveProps'}}
  /**
  * {{this}}
  */
  {{this}}(){}
  
{{/eq}}{{/each}}{{/if}}
{{#each requiredFunctions}}
  /**
  * {{this}}
  * {{#eq func 'constructor'}}@param {object} props incoming props{{/eq}}
  */
  {{this}}({{#eq func 'constructor'}}props{{/eq}}){
{{#eq func 'constructor'}}
    super(props);
{{/eq}}
  }

{{/each}}
  /**
  * render
  * @returns {XML} The component
  */
  render() {
    return (
{{#eq styleType 'SCSS'}}
      <div>
{{/eq}}
{{#eq styleType 'Styled Components'}}
      <{{pascalCase name}}>

{{/eq}}
{{#eq styleType 'Material design'}}
      <{{pascalCase name}}>
{{/eq}}
        <p>{{pascalCase name}} Component</p>
{{#eq styleType 'SCSS'}}
      </div>
{{/eq}}
{{#eq styleType 'Styled Components'}}
     </{{pascalCase name}}>

{{/eq}}
{{#eq styleType 'Material design'}}
     </{{pascalCase name}}>
{{/eq}}
    );
  }
}
{{/eq}}
