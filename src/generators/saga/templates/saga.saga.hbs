import { put, takeLatest } from 'redux-saga/effects';

import actions from './{{camelCase name}}.actions';

{{#each actionConstants}}
/**
 *
 * @param {object} params Params
 */
function* {{camelCase this}}() {
{{#each reducerConstants}}
  yield put({
    type: actions.{{this}},
    payload: {
      test: '123',
    },
  });
{{/each}}
  return;
}
{{/each}}

/**
 *
 */
export default [
{{#each actionConstants}}
  takeLatest(actions.{{this}}, {{camelCase this}}),
{{/each}}
]
