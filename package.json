  "name": "goodz",
  "description": "A command-line utility for generating goodness",
  "version": "1.0.6",
  "author": "Goods & Services",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goods-services/goodz.git"
  },
  "bugs": {
